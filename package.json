{
  "name": "stavrosliaskos",
  "version": "3.2.1",
  "description": "My Nuxt.js based developer website",
  "author": "Stavros Liaskos",
  "homepage": "https://stavrosliaskos.com/",
  "repository": "https://github.com/stavros-liaskos/stavrosliaskos",
  "keywords": [
    "nuxt",
    "stavros",
    "liaskos",
    "stavrosliaskos",
    "developer"
  ],
  "bugs": "https://github.com/stavros-liaskos/stavrosliaskos/issues",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "image": "bash image_cropper.sh",
    "hint": "./node_modules/hint/dist/src/bin/hint.js https://stavrosliaskos.com",
    "precommit": "yarn lint",
    "postcommit": "yarn deploy",
    "test": "ava --serial --verbose",
    "deploy": "yarn lintfix && now rm -y stavrosliaskos && now && now alias",
    "favicon-gen": "real-favicon generate faviconDescription.json faviconData.json static/favicon"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "nuxt": "^2.2.0",
    "vue-lazyload": "^1.2.6",
    "vue-particles": "^1.0.9",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.13.0",
    "vue-typer": "^1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/sitemap": "^0.1.1",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "breakpoint-sass": "^2.7.1",
    "cli-real-favicon": "0.0.6",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "fontfaceobserver": "^2.0.13",
    "glob-all": "^3.1.0",
    "hint": "^4.0.1",
    "jsdom": "^12.1.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.6",
    "nuxt-sass-resources-loader": "^2.0.5",
    "prettier": "1.15.2",
    "purgecss-webpack-plugin": "^1.3.1",
    "sass-loader": "^7.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
