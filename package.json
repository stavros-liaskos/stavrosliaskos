{
  "name": "stavrosliaskos",
  "version": "4.1.0",
  "description": "My Nuxt.js based developer website",
  "author": "Stavros Liaskos",
  "homepage": "https://stavrosliaskos.com/",
  "repository": "https://github.com/stavros-liaskos/stavrosliaskos",
  "keywords": [
    "nuxt",
    "stavros",
    "liaskos",
    "stavrosliaskos",
    "developer"
  ],
  "bugs": "https://github.com/stavros-liaskos/stavrosliaskos/issues",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint -- .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "image": "bash image_cropper.sh",
    "hint": "./node_modules/hint/dist/src/bin/hint.js https://stavrosliaskos.com",
    "precommit": "yarn lint",
    "postcommit": "yarn deploy",
    "deploy": "yarn lintfix && now rm -y stavrosliaskos && now",
    "now-build": "nuxt build --spa",
    "favicon-gen": "real-favicon generate faviconDescription.json faviconData.json static/favicon"
  },
  "dependencies": {
    "cross-env": "7.0.0",
    "nuxt": "2.11.0",
    "vue-lazyload": "1.3.3",
    "vue-particles": "1.0.9",
    "vue-router": "3.1.5",
    "vue-scrollto": "2.17.1",
    "vue-typer": "1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/sitemap": "2.0.1",
    "babel-eslint": "10.0.3",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "breakpoint-sass": "2.7.1",
    "cli-real-favicon": "0.0.8",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.1.2",
    "fontfaceobserver": "2.1.0",
    "glob-all": "3.1.0",
    "hint": "6.0.3",
    "node-sass": "4.13.1",
    "nodemon": "2.0.2",
    "nuxt-sass-resources-loader": "2.0.5",
    "prettier": "1.19.1",
    "purgecss-webpack-plugin": "2.2.0",
    "sass-loader": "8.0.2",
    "vue-svg-loader": "0.16.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
